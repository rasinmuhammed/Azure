{
	"name": "DF_raw_to_prep_misumi",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DS_SQL_misumi",
						"type": "DatasetReference"
					},
					"name": "Category"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_misumi",
						"type": "DatasetReference"
					},
					"name": "Navigation"
				},
				{
					"dataset": {
						"referenceName": "DS_SQL_misumi",
						"type": "DatasetReference"
					},
					"name": "Products"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {Parent Category} as string,",
				"          Subcategory as string,",
				"          Link as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Category",
				"source(output(",
				"          {Parent Category} as string,",
				"          Subcategory as string,",
				"          Link as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Navigation",
				"source(output(",
				"          {Brand Name} as string,",
				"          {Category Name} as string,",
				"          {Max Price Per Piece} as string,",
				"          {Max Standard Days to Ship} as string,",
				"          {Max Standard Unit Price} as string,",
				"          {Min Price Per Piece} as string,",
				"          {Min Standard Days to Ship} as string,",
				"          {Min Standard Unit Price} as string,",
				"          {Page Views} as string,",
				"          {Price Check Less Flag} as string,",
				"          {Product Image URL} as string,",
				"          {Product Name} as string,",
				"          {Recommend Flag} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Products",
				"Products select(mapColumn(",
				"          {Brand Name},",
				"          {Category Name},",
				"          {Max Price Per Piece},",
				"          {Max Standard Days to Ship},",
				"          {Max Standard Unit Price},",
				"          {Min Price Per Piece},",
				"          {Min Standard Days to Ship},",
				"          {Min Standard Unit Price},",
				"          {Page Views},",
				"          {Product Image URL},",
				"          {Product Name}",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 derive({Max Price Per Piece} = toFloat({Max Price Per Piece}),",
				"          {Max Standard Days to Ship} = toInteger({Max Standard Days to Ship}),",
				"          {Max Standard Unit Price} = toFloat({Max Standard Unit Price}),",
				"          {Min Price Per Piece} = toFloat({Min Price Per Piece}),",
				"          {Min Standard Days to Ship} = toInteger(Min)) ~> derivedColumn1"
			]
		}
	}
}